<!-- Modal -->
<div id="pembelianModal" class="modal fade" role="dialog">
  <div class="modal-dialog">
    <!-- Modal content-->
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal">
          &times;
        </button>
        <h4 class="modal-title">Pilih Suplier</h4>
      </div>
      <div class="modal-body" style="overflow: scroll">
        <table id="tableSuplier" class="table table-striped table-bordered">
          <thead>
            <th width="10%">No</th>
            <th>Nama</th>
            <th>Alamat</th>
            <th>Telepon</th>
            <th><i class="fa fa-fw fa-gear"></i></th>
          </thead>
        </table>
      </div>
    </div>
  </div>
</div>
